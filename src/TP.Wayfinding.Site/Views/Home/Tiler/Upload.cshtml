
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Upload Tile</title>
    <link rel="stylesheet" href="~/Content/tiler.css">
    <script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?sensor=false&libraries=places"></script>
    <script type="text/javascript" src="~/Scripts/tests/upload.js"></script>
</head>
<body>
    <p>Upload an image to overlay:</p>
    <form action="@Url.Action("EditTiler", "Home")" method="get" enctype="multipart/form-data">
        <input type="text" name="overlay" value="http://www.google.com/intl/en_com/images/srpr/logo3w.png">
        <input type="submit" value="Upload">
        <input type="hidden" name="lat" id="lat">
        <input type="hidden" name="lng" id="lng">
        <input type="hidden" name="z" id="zoom">
    </form>
    Choose initial position:
    <div id="upload-map"></div>
</body>
</html>
