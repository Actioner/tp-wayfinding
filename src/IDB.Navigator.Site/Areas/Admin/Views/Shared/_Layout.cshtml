<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="TP Navigator">
    <meta name="author" content="Jorge Rodriguez Breuning, Ignacio Raul Cerrato">
    <meta name="keyword" content="Indoor, Map, Navigator, Navigation, GPS">
    <link rel="shortcut icon" href="~/Content/karmanta/img/favicon.png">
    <title>@(ViewBag.Title + " - Navigator")</title>

    @Styles.Render("~/Content/karmanta/css")
    @RenderSection("styles", required: false)

    @Scripts.Render("~/bundles/modernizr")

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 -->
    <!--[if lt IE 9]>
    <script src=""~/Scripts/html5shiv.js"></script>
    <script src=""~/Scripts/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <section id="container" class="">

        @Html.Partial("Header/_Header")
        @Html.Partial("Aside/_Aside")

        <section id="main-content">
            <section class="wrapper">
                @RenderBody()
            </section>
        </section>
    </section>

    @RenderSection("dialogs", required: false)

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/karmanta/js")
    @RenderSection("scripts", required: false)
</body>
</html>
