@{
    ViewBag.Title = "Edit Floor";
}

<div class="row">
    <div class="col-lg-12">
        @Html.EditorForModel()
    </div>
</div>

@section Scripts {
    <script type="text/javascript" src="https://maps.google.com/maps/api/js?sensor=true&libraries=places"></script>
    @Scripts.Render("~/bundles/jqueryval")
    <script type="text/javascript" src="~/Scripts/wayfinding/building.js"></script>

    @if (TempData["NotifyCreation"] != null && (bool)TempData["NotifyCreation"])
    {
        <script type="text/javascript">
            $(function () {
                window.notifyCreate();
            });
        </script>
    }
}
